<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Label="UserMacros" />
  <PropertyGroup>
    <_PropertySheetDisplayName>pixmanprops</_PropertySheetDisplayName>
    <TargetName>$(ProjectName)$(LibSuffix)-vc$(PlatformToolsetVersion)</TargetName>
    <OutDir>$(OutputBase)/pixman/bin/</OutDir>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <PreprocessorDefinitions>_CRT_SECURE_NO_DEPRECATE;_CRT_NONSTDC_NO_DEPRECATE;PACKAGE="pixman-1";PACKAGE_VERSION="";PACKAGE_BUGREPORT="";USE_SSE2;USE_MMX;%(PreprocessorDefinitions)</PreprocessorDefinitions>
      <DisableSpecificWarnings>4819;4244;%(DisableSpecificWarnings)</DisableSpecificWarnings>
      <CompileAs>CompileAsC</CompileAs>
      <AdditionalIncludeDirectories>$(W32DepsDir)/pixman/include/</AdditionalIncludeDirectories>
    </ClCompile>
    <Link>
      <ImportLibrary>$(OutputBase)/pixman/lib/$(TargetName).lib</ImportLibrary>
    </Link>
  </ItemDefinitionGroup>
  <ItemGroup />
</Project>